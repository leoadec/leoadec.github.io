{
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "sass": "^1.83.4"
  },
  "scripts": {
    "check": "npm run check-css && npm run check-html",
    "check-css": "java -jar $(node vnu.js) --skip-non-css _site/",
    "check-html": "java -jar $(node vnu.js) --skip-non-html _site/",
